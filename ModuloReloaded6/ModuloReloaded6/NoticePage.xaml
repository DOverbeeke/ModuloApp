<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ModuloReloaded6.NoticePage">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
      <Label x:Name="Title" FontAttributes="Bold" FontSize="Large"/>
      <Label x:Name="Date" FontAttributes="Italic" FontSize="Small"/>
      <Label x:Name="Message" FontSize="Medium"/>
      <Label x:Name="Status" FontSize="Small"/>
    </StackLayout>
    </ContentPage.Content>
</ContentPage>